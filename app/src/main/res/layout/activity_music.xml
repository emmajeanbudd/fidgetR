    <ImageButton
        android:id="@+id/fire"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/fire"
        android:background="@null"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="7dp"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/ocean"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/ocean" />

    <ImageButton
        android:id="@+id/forest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:background="@null"
        app:srcCompat="@drawable/forest"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="16dp" />

    <ImageButton
        android:id="@+id/ocean"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ocean"
        android:background="@null"
        app:layout_constraintRight_toLeftOf="@+id/forest"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="2dp"
        android:layout_marginTop="17dp"
        app:layout_constraintTop_toBottomOf="@+id/fire"
        android:layout_marginRight="2dp" />

    <ImageButton
        android:id="@+id/piano"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/piano"
        android:background="@null"
        android:layout_marginEnd="16dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/forest"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/forest"
        android:layout_marginRight="16dp" />

    <ImageButton
        android:id="@+id/rain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/rain"
        android:background="@null"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:id="@+id/wind"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:srcCompat="@drawable/wind"
        android:background="@null"
        android:layout_marginEnd="16dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="17dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/ocean"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="6dp"
        app:layout_constraintLeft_toRightOf="@+id/ocean"
        android:layout_marginLeft="17dp"
        android:layout_marginRight="16dp" />
</android.support.constraint.ConstraintLayout>
